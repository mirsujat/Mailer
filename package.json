{
  "name": "server",
  "version": "1.0.0",
  "description": "App with React Redux Express and MongoDB",
  "main": "index.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run webhook\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "webhook": "forever sendgrid_webhook.js"
  },
  "author": "mirsujat",
  "license": "ISC",
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-session": "~2.0.0-beta.3",
    "express": "~4.16.2",
    "localtunnel": "~1.8.3",
    "lodash": "~4.17.4",
    "mongoose": "~4.13.7",
    "nodemon": "~1.12.6",
    "passport": "~0.4.0",
    "passport-google-oauth20": "~1.0.0",
    "path-parser": "~3.0.1",
    "sendgrid": "~5.2.3",
    "stripe": "~5.4.0"
  },
  "devDependencies": {
    "forever": "~0.15.3"
  }
}
